void setup(){
  size(550, 550);
  background(255);
  frameRate(15);
  
}
void draw(){
  background(255);

  int y=0;
  while (y<506){
    int x=0;
    while (x<506){
      stroke(0);
      rectMode(CENTER);
      fill(255);
       float rand= random(0,50);
  if (rand<35){
      line(22+x, 22+y, 22+x+6, 22+y);
      line(x+25, y+25, x+25, y+19);
   
  }
     x+=25;
    }
    y+=25;
 
  }
  
}


